/* 1st code 8/

public class Employee {
    int employeeId; 
    int salary;
    String dept;
    
    Employee (int empid, int sal, String d){
        employeeId=empid; 
        salary=sal;
        dept =d;
    }
    public static void main(String[] args) {
        Employee harsha = new Employee (101,3000, "Testing");
        Employee varsha = new Employee (10p, 5000, "Engineering");
        System.out.printin (harsha.employeeId+" Salary "+harsha.salary+" Dept "+harsha.dept);
        System.out.println(varsha.employeeId+" Salary "+varsha.salary+" Dept "+varsha.dept);
    }
}
